{
  "name": "kick-plugin-v2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {},
  "scripts": {
    "start": "parcel src/popup/popup.html",
    "build": "run-s cpy build:parcel",
    "build:parcel": "parcel build src/popup/popup.html --no-content-hash ",
